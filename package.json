{
  "name": "portfolio",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "dev:setup": "pnpm install; pnpm prepare; pnpm playwright install",
    "format": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
